<script>
  function showEvents(data) {
    // code to process results from server
    // and display it in the page here

    $('selector').on('load', function(img){
      img.preventDefault();
      var queryString = "src="" + event.target.id """;
      $.get("chucksserver.com", queryString, function(data){
        $('#basic-cuts-api').html(data);
      } );
    });

  };

</script>

